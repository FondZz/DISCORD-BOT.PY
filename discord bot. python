@client.command()
async def kick(ctx, member : discord.Member, *, reason=None):
      await member.kick(reason=reason)


@client.command()
async def ban(ctx, member: discord.Member, *, reason=None):
    await member.ban(reason=reason)
    await ctx.send(f'BANNED {member.mention}')

@client.command()
async def unban(ctx, *, member):
    banned_users  = await ctx.guild.bans()
    member_name, member_discriminator = member.split('#')

    for ban_entery in banned_users:
        user = ban_entery.user


        if (user.name, user.discriminator) == (member_name, member_discriminator):
            await ctx.guild.unban(user)
            await ctx.send(f'UNBANNED {user.mention}')
            return




@client.event

async def on_ready():
      await client.change_presence(status = discord.Status.idle, activity = discord.Game('TWOMAD'))
      print('bot')



@client.event
async def on_command_error(ctx, error):
 if isinstance(error, commands.CommandNotFound): await ctx.send('Okay listen here pal, i wasnt made for you to use me for no reason. And shut im trying to dm MEE6.')



@client.command()
async def clear(ctx, amount : int):
        await ctx.channel.purge(limit=amount)


@clear.error
async def clear_error(ctx, error):
    if isinstance(error, commands.MissingRequiredArgument):
        await ctx.send('Please specify an amount of messages to delete')



@client.command()
async def mute(ctx, member:discord.Member):
    role = discord.utils.get(ctx.guild.roles, name ="Muted")
    guild = ctx.guild
    if role not in guild.roles:
        perms = discord.permissions(send_messages=False, speak= False)
        await guild.creat_role(name="Muted")
        await member.add_roles(role)
        await ctx.send(f'{member} was muted')
    else:
        await member.add_roles(role)
        await ctx.send(f'{member} was muted')


async def unmute(ctx, member:discord.Member):
    role = discord.utils.get(ctx.guild.roles, name ="un mute")
    guild = ctx.guild
    if role not in guild.roles:
        perms = discord.permissions(send_messages=True, speak= True)
        await guild.creat_role(name="un mute")
        await member.add_roles(role)
        await ctx.send(f'{member} was UN muted')
    else:
        await member.add_roles(role)
        await ctx.send(f'{member} was UN muted')



