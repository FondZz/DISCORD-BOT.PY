@client.command()
async def addrole(ctx, role: discord.Role, user: discord.Member):
    if ctx.aurthor.guild_permissions.administrator:
        await user.add_roles(role)
        await ctx.send(f"TAKE THIS ROLE {user}")


def sub(x: float, y: float):
    return x - y


def add(x:float,y: float):
    return x + y


def div(x:float,y: float):
    return x / y


def rando(x:float,y: float):
    return random.randint(x.y)


def sqrt(x:float):
    return math.sqrt(x)


@client.command()
async def mathsqrt(ctx, x: int, y: int):
    res = sqrt(x)
    await ctx.send(res)

@client.command()
async def mathsqrte(ctx, x: int, y: int):
    res = sqrt(x)
    await ctx.send(res)


@client.command()
async def ping(ctx):
    await ctx.send(f'Pong! {round(client.latency * 1000)}ms')






